<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="eu.licentia.necessitas.poc.appv1"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="4" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name="eu.licentia.necessitas.poc.apptemplate.StarterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name="eu.licentia.necessitas.poc.apptemplate.AppTemplate" android:label="@string/app_name">
            <intent-filter>
                <action android:name="eu.licentia.necessitas.poc.apptemplate.AppTemplate"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

    </application>
</manifest>